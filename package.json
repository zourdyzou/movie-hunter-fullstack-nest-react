{
  "name": "movie-hunter",
  "description": "Movie information for people who loves movie and curious about their movie favourites",
  "version": "1.0.0",
  "private": true,
  "author": "Muhammad Zourdy <hello@zourdyzou.dev> (@zourdyzou)",
  "license": "MIT",
  "keywords": [
    "Nextjs",
    "typescript",
    "Cinema Information",
    "Nest.js"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.1",
    "@tailwindcss/typography": "0.5.2",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "draft-js": "0.11.7",
    "draftjs-to-html": "0.9.1",
    "html-react-parser": "1.4.12",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "sass": "1.51.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/node": "16.11.43",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "autoprefixer": "10.4.7",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "postcss": "8.4.38",
    "postcss-easy-import": "4.0.0",
    "postcss-nested": "5.0.6",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
